{
    "name": "llylim/freshdesk-php-sdk",
    "description": "Customized PHP SDK for the Freshdesk API (v2)",
    "type": "library",
    "homepage": "https://github.com/llylim/freshdesk-php-sdk",
    "keywords": ["freshdesk", "customer service", "support", "help desk", "api"],
    "license": "MIT",
    "authors": [
        {
            "name": "Willy Salim",
            "email": "willzsalim.lim@gmail.com",
            "homepage": "https://github.com/llylim"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": ">=4.0,<7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "evert/phpdoc-md" : "~0.2.0"
    },
    "autoload": {
        "psr-4": {"Freshdesk\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Freshdesk\\tests\\": "tests/" }
    }
}
